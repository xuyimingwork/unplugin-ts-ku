{
  "name": "unplugin-ts-ku",
  "type": "module",
  "version": "0.0.3",
  "description": "a tool to generate ts string literal union type from files",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch src",
    "build": "tsup",
    "prepublish": "pnpm build"
  },
  "keywords": [
    "unplugin"
  ],
  "author": "Xu Yiming",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xuyimingwork/unplugin-ts-ku.git"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.5.3",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vite": "^5.4.2"
  },
  "dependencies": {
    "@antfu/utils": "^0.7.10",
    "debug": "^4.4.0",
    "fast-glob": "^3.3.2",
    "lodash-es": "^4.17.21",
    "minimatch": "^10.0.1",
    "unplugin": "^1.12.3"
  }
}
